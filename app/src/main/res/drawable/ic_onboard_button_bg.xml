<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="1dp"
        android:right="1dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="360"
                android:startColor="#776AFF"
                android:endColor="#A59CFF"
                android:type="linear" />
            <corners android:radius="48dp" />
        </shape>
    </item>

</layer-list>
